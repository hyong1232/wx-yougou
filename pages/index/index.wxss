.index_swipper swiper {
  width: 100%;
  height: calc(340 * 100vw / 750);
}
.index_swipper swiper image {
  width: 100%;
}
.index_cates {
  display: flex;
}
.index_cates navigator {
  flex: 1;
  padding: 20rpx;
}
.index_cates navigator image {
  width: 100%;
}
.index_floor .floor_group .floor_title {
  padding: 10px 0;
}
.index_floor .floor_group .floor_title image {
  width: 100%;
}
.index_floor .floor_group .product_list {
  overflow: hidden;
}
.index_floor .floor_group .product_list navigator {
  float: left;
  width: 33.33%;
}
.index_floor .floor_group .product_list navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #FFF;
}
.index_floor .floor_group .product_list navigator:nth-child(2),
.index_floor .floor_group .product_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #FFF;
}
.index_floor .floor_group .product_list navigator image {
  width: 100%;
  height: 100%;
}
